<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="45dp"
    android:background="@drawable/point_bg_l"
    android:layout_gravity="center"
    android:orientation="horizontal" >
    
    <LinearLayout
        android:id="@+id/orange_one" 
        android:layout_width="46dp"
        android:layout_height="40dp"
        android:background="@drawable/point_client_l"
        android:orientation="horizontal"
        android:layout_marginTop="2dp"
        android:gravity="center">
        
        <Button 
	        android:id="@+id/button_l_1"
	        style="@style/Sj_MyButton"
	        android:layout_width="40dp"
	        android:layout_height="40dp"
	        android:background="@drawable/point_client_style"/>
        
    </LinearLayout>
    <Button 
        android:id="@+id/button_l_2"
        style="@style/Sj_MyButton"
        android:layout_marginTop="1dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/point_ac_style"/>
    
    <Button 
        android:id="@+id/button_l_3"
        style="@style/Sj_MyButton"
        android:layout_marginTop="1dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/point_ser_style"/>
    
    <Button 
        android:id="@+id/button_l_4"
        style="@style/Sj_MyButton"
        android:layout_marginTop="1dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/point_gift_style"/>
    
    <Button 
        android:id="@+id/button_l_5"
        style="@style/Sj_MyButton"
        android:layout_marginTop="1dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/point_jin_style"/>
</LinearLayout>
